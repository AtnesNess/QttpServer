{
    "bindIp": "0.0.0.0",
    "bindPort": 8080,
    "server": {
        "strictHttpMethod": false,
        "metadata": true,
        "processors": {
            "options" : true
        }
    },
    "logfile": {
        "isEnabled": true,
        "filename": null,
        "writeFrequency": 300
    },
    "swagger": {
        "isEnabled": true,
        "version": "0.1",
        "description": "QttpServerDescription",
        "title": "QttpServerTitle",
        "host": "127.0.0.1:8080",
        "basePath": "/"
    },
    "httpFiles": {
        "isEnabled": true,
        "directory": "/usr/local/qttpfiles"
    },
    "defaultHeaders" : {
        "Content-Type": "application/json",
        "Access-Control-Allow-Headers": "Content-Type, Authorization" ,
        "Access-Control-Allow-Methods": "GET, POST, DELETE, PUT, PATCH, HEAD, OPTIONS, TRACE" ,
        "Access-Control-Allow-Origin": "*"
    }
}
